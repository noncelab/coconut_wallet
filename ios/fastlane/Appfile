apple_id(ENV['FASTLANE_USER'])                      # ex) you@example.com
itc_team_id(ENV['APP_STORE_CONNECT_TEAM_ID'])       # App Store Connect Team ID
team_id(ENV['DEVELOPER_PORTAL_TEAM_ID'])            # Developer Portal Team ID (Certificates)

